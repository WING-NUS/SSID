# Ignore database config file
/config/database.yml

# Ignore passenger restart.txt
/tmp/restart.txt

# Ignore swap files
*.swp

# Ignore mysqldb connector
/lib/java/programs/lib/*

# Ignore upload and upload_log directories
/upload
/upload_log

# Ignore bundler config
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# Ignore mysql-connector-java-5.1.13-bin.jar
/lib/java/programs/lib/
/lib/java/DBSCAN/lib/
/lib/java/PlagiarismDetection/lib/
